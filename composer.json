{
	"name": "raymond-byczko/php-codfish-website",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/RaymondByczko/php-codfish"
		}
	],
	"autoload": {
        "psr-4": {
            "RaymondByczko\\PhpCodfishWebsite\\":"src/"
        }
    },

	"require": {
		"raymond-byczko/php-codfish":"dev-master"
	},
	"require-dev": {
		"raymond-byczko/php-codfish":"dev-master",
		"phpunit/phpunit": "^8.2"
	},
	"bin": ["bin/codfish-website-install.php"],
	"minimum-stability": "dev",
	"prefer-stable": true
}
